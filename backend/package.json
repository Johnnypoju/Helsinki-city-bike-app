{
  "name": "helsinki-city-bike-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node ./build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "db:reset": "npx sequelize-cli-typescript db:drop && npx sequelize-cli db:create && npx sequelize-cli-typescript db:migrate && npx sequelize-cli-typescript db:seed:all",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli-typescript db:create",
    "build": "tsc",
    "migration:down": "ts-node ./util/rollback.ts"
  },
  "author": "Joonas Sumusalo",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "sequelize-cli-typescript": "^3.2.0-c",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv": "^6.3.0",
    "dotenv": "^16.0.3",
